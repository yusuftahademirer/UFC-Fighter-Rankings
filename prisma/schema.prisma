generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Fighter {
  id          Int      @id @default(autoincrement())
  name        String
  weightClass String
  ranking     String?
  pfpRanking  Int?
  createdAt   DateTime @default(now())
}

model User {
  id       String    @id
  email    String    @unique
  fighters Fighter[]
}